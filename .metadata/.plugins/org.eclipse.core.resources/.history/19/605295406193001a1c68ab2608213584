#include <stdio.h>
#include "includes.h"

// stacks
#define   TASK_STACKSIZE       2048

OS_STK    task_adc_to_fft_stack[TASK_STACKSIZE];

// tasks
void task_adc_to_fft(void *pdata);

int main(void) {

  OSInit();

  OSTaskCreateExt(task_adc_to_fft, (void *) 0, &task_adc_to_fft_stack[TASK_STACKSIZE - 1], 4);



  OSStart();
  return 0;
}

void task_adc_to_fft(void* pdata) {

  while (1) {

  }
}


